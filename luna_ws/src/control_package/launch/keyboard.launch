<launch>
  <param name="robot_description" command="$(find xacro)/xacro --inorder '$(find luna_rviz)/urdf/robot.urdf'"/>
  <node
    pkg="robot_package"
    type="listener.py"
    name="listener" 
    output="screen"
  />
  <node
    pkg="control_package"
    type="keyboard_control.py"
    name="keyboard_control" 
    output="screen"
  />
  <node 
    type="rviz" 
    name="rviz" 
    pkg="rviz" 
    args="-d $(find luna_rviz)/rviz/luna_config.rviz" 
  />
  <node
    pkg="localization_package"
    type="image_subscriber.py"
    name="image_subscriber" 
    output="screen"
  />
  <node
    pkg="control_package"
    type="autonomous_control.py"
    name="autonomous_control" 
    output="screen"
  />
  <node
    pkg="mapping_package"
    type="map.py"
    name="map" 
    output="screen"
  />
</launch> 